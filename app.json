{
	"expo": {
		"name": "cuokka",
		"slug": "cuokka",
		"version": "1.0.0",
		"scheme": "cuokka",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.yourcompany.cuokka",
			"infoPlist": {
				"CFBundleURLTypes": [
					{
						"CFBundleURLSchemes": ["cuokka"]
					}
				]
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"permissions": ["INTERNET"],
			"package": "com.simone.cuokka",
			"intentFilters": [
				{
					"action": "VIEW",
					"data": [
						{
							"scheme": "cuokka"
						}
					],
					"category": ["BROWSABLE", "DEFAULT"]
				}
			],
			"usesCleartextTraffic": true,
			"compileSdkVersion": 33,
			"targetSdkVersion": 33
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"extra": {
			"backendUrl": "http://88.99.2.164:3002/api",
			"eas": {
				"projectId": "c29d42ae-529d-49bb-88cb-3a1fa08a5cf2"
			}
		},
		"owner": "kevinlee715"
	}
}
